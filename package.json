{
  "name": "cycle-import-check",
  "version": "1.0.4",
  "description": "ES6 cycle import check tool, support js, ts, jsx, tsx and mjs files",
  "bin": {
    "iscan": "lib/cli.js"
  },
  "devDependencies": {
    "@types/cli": "^0.11.19",
    "@types/glob": "^5.0.35",
    "@types/graphlib": "^2.1.4",
    "@types/jest": "^20.0.7",
    "@types/lodash": "^4.14.102",
    "@types/node": "^8.0.25",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.4.2",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "release": "standard-version && npm pack",
    "prepare": "npm run build && npm test"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "cli": "^1.0.1",
    "colors": "^1.3.0",
    "glob": "^7.1.2",
    "graphlib": "^2.1.5",
    "lodash": "^4.17.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/tests/",
      "/node_modules/",
      "/src/cli"
    ],
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun@sap.com",
    "url": "https://github.com/Soontao"
  },
  "main": "lib/index.js",
  "license": "MIT"
}
